{
    "$schema" : "http://json-schema.org/schema#",
    "description" : "Map from option id to block id (i.e. string to string)",
    "type" : "object",
    "id" : "#branchMap",
    "patternProperties" : {
        "^comp_[0-9]+_[0-9]+" : {
            "type" : "string",
            "pattern" : "[1-9]+[0-9]*(\\.(_?|[a-z]?)[1-9]+[0-9]*)*"
        }
    }
}